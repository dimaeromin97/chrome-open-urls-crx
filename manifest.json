{
	"manifest_version": 2,
	"name": "Open URLs",
	"description": "Open selected URLs",
	"version": "0.0.1",
	"browser_action": {
		"default_icon": "img/128.png"
	},
	"icons": {
		"128": "img/128.png",
		"19": "img/19.png"
	},
	"background": {
		"scripts": [
			"dist/bundle.js"
		]
	},
	"permissions": [
		"background",
		"contextMenus",
		"activeTab",
		"<all_urls>",
		"tabs"
	]
}