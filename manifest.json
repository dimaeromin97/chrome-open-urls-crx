{
	"manifest_version": 2,
	"name": "openURLs",
	"short_name": "openURLs",
	"description": "Open selected URLs",
	"version": "0.0.2",
	"browser_action": {
		"default_icon": "img/512.png"
	},
	"icons": {
		"512": "img/512.png",
		"128": "img/128.png",
		"16": "img/16.png",
		"19": "img/19.png"
	},
	"background": {
		"scripts": [
			"dist/bundle.js"
		]
	},
	"permissions": [
		"contextMenus",
		"activeTab",
		"tabs"
	]
}